{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Tehnut",
    "JoulsMB/WonejoMB"
  ],
  "contact": {
    "sources": "https://github.com/JoulsMB/GuidebookAPI"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "mixins": [
    {
      "config": "guidebookapi.mixins.json",
      "environment": "*"
    }
  ],
  "entrypoints": {
    "main": [
      "de.mrbunny.guidebook.GuidebookMod"
    ],
    "client": [
      "de.mrbunny.guidebook.client.GuidebookClientMod"
    ],
    "guidebookapi": [
      "de.mrbunny.guidebook.example.ExampleBook"
    ]
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_range}",
    "fabric-api": ">=0.96.3+1.20.4",
    "minecraft": "${mc_version}"
  }
}
